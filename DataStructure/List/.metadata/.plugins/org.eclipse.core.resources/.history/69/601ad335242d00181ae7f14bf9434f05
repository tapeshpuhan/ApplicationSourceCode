#ifndef TREE_H_
#define TREE_H_

#include<iostream>
#include<string>
using namespace std;

template<typename T>
class BSt
{
	using tNode = struct Node
			{
				Node *Left;
				Node *Right;
				T data;
				Node():Left(nullptr),Right(){}
			};
}



#endif /* TREE_H_ */
