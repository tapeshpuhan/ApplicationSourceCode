#include "XmlParser.h"
#include<istream>
#include <fstream>

void XmlPars::ParseXml(const string &path)
{
	ifstream file(path);

	if(!file.is_open())
	{
		cout<<"Error: file not found"<<endl;
	}
}

int main()
{
	return 0;
}

