#include "XmlParser.h"
#include<istream>

void XmlPars::ParseXml(const string &path)
{
	ifstream file(path);
	if(!file.is_open())
	{
		cout<<"ERROR: Error in File opening"<<path.c_str();
		return;
	}
}

int main()
{
	return 0;
}

