#include "BaseFile.h"

void test1()
{
	vector<int> v={2,3,4,5,6};

	if(all_of(v.cbegin(),v.cend(),[&](const int &i){return (i>=2);}))
	{
		cout<<"All values are greter than 2"<<endl;
	}
}


int main()
{
	test1();
return 0;
}
