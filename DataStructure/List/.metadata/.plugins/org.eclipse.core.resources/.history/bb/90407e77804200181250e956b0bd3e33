#include<set>
#include<iostream>
#include"Task.h"
using namespace std;

template<typename T>
class lesser_t
{
public:
	bool operator ()(const T &a, const T &b)
	{
		return a< b;
	}
};

template<typename T>
class greater_t
{
public:
	bool operator ()(const T &a, const T &b)
	{
		return a > b;
	}
};

int main()
{

	return 0;
}
