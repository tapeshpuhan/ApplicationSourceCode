#include<iostream>
#include<vector>
#include <map>
#include <set>
#include <list>
#include <cmath>
#include <ctime>
#include <deque>
#include <queue>
#include <stack>
#include <string>
#include <bitset>
#include <cstdio>
#include <limits>
#include <vector>
#include <climits>
#include <cstring>
#include <cstdlib>
#include <fstream>
#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>
#include <unordered_map>
using namespace std;

bool isFormatOk(string str)
{
	bool found = true;
	const set<char> Spair={'{','[','('};
	map<char,char> Tpair = {{'}','{'},{']','['},{')','('}};
	stack<char> Store;

	for(auto &at : str)
	{
		if(Spair.find(at)!=Spair.end())
		{
			Store.push(at);
		}
		else if(!Tpair[at])
		{
			if(Tpair[at]!=Store.top())
			{
				return false;
			}
			if(!Store.empty())
			{
				Store.pop();
			}
		}
		else
		{
			return false;
		}
	}

	return found;
}

int main()
{
	return 0;
}

