#include<set>
#include<iostream>
using namespace std;

template<typename T>
class lesser_t
{
public:
	bool operator<(const T &a, const T &b)const
	{
		return a< b;
	}
};

template<typename T>
class greater_t
{
public:
	bool operator>(const T &a, const T &b)const
	{
		return a > b;
	}
};

