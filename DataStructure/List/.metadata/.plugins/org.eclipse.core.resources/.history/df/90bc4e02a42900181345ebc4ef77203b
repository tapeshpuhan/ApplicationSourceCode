#include<iostream>
using namespace std;

void testAutoType()
{
	int x=0; //declare x type
	const int &crx=x; //declare const ref type
	auto aCrx = crx;

	x = 42;
	cout<<"Before modification aCrx "<<aCrx<<" crx "<<crx<<" x "<<x<<endl;
}

int main()
{
	testAutoType();
	return 0;
}



