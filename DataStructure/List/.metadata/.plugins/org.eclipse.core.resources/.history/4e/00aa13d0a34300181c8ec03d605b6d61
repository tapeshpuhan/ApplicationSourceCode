#include<iostream>
#include<string>
using namespace std;


void fun(int &a)
{
	cout<<"fun "<<a<<endl;
}
void fun(const int &a)
{
	cout<<"const fun "<<a<<endl;
}

void fun( int &&a)
{
	cout<<"const fun "<<a<<endl;
}
int main()
{
	int a = 5;
	fun(5);
	fun(a);
	return 0;
}


