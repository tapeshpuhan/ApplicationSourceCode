#include<set>
#include<iostream>
#include"Task.h"
using namespace std;

template<typename T>
class lesser_t
{
public:
	bool operator ()(const T &a, const T &b)
	{
		return a< b;
	}
};

template<typename T>
class greater_t
{
public:
	bool operator ()(const T &a, const T &b)
	{
		return a > b;
	}
};

int main()
{
	set<int, greater_t<int>> _set = {100,200,23,45,234,221,45,3,3,2,224,45,2,4,5,3,56,34,3};

	PRINT(_set);
	auto pos = _set.upper_bound(3);
	cout<<*pos;
	//PRINT(pos);
	return 0;
}
