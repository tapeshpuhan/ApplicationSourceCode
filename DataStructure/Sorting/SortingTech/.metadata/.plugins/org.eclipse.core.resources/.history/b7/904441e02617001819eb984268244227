#ifndef SEARCHING_H_
#define SEARCHING_H_
#include "Sorting.h"

template<typename T>
class Searching
{
public:

	void Insert(T data)
	{
		_data = data;
	}

	virtual int Search(T value)=0;
	virtual ~Searching(){_data.clear();}
protected:
	vector<T> _data;
};



#endif /* SEARCHING_H_ */
