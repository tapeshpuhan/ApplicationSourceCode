#ifndef SEARCHING_H_
#define SEARCHING_H_

template<typename T>
class Searching
{
public:

	void Insert(T data);


	virtual int Search(T value)=0;
	virtual ~Sorting(){_data.clear();}
protected:
	vector<T> _data;
};



#endif /* SEARCHING_H_ */
