#include "Searching.h"

template<typename T>

int BinarySearch<T>::Search(T value)
{
	int po = 0;
	int mid = Searching<T>::_data.size()/2;
	int size = Searching<T>::_data.size();
	bool found = false;


	while(mid)
	{
		if(value > Searching<T>::_data[mid])
		{
			po = mid+1;
		}
	}

	return po;
}


